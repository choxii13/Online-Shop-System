<tr data-csrf="<%=locals.csrfToken%>" data-productId="<%=cartItem.product.id%>">
  <td>
    <div class='product'>
      <img src='<%=cartItem.product.imageUrl%>' />
      <h5 class="title"> <%= cartItem.product.title %></h5>
      <h5 class="shortenTitle"> <%=cartItem.product.shortenTitle %></h5>
    </div>
  </td>
  <td>
    <h5> ₱ <%= cartItem.product.price.toFixed(2) %> </h5>
  </td>
  <td>
    <div class='shopping-cart-quantity'>
      <button class='bi bi-plus-square-fill add'>
      </button>
      <span class="cartQuantity"><%= cartItem.quantity %></span>
      <button class='bi bi-dash-square-fill minus'> </button>
    </div>
  </td>
  <td>
    <h5 class="total-price"> ₱ <%= cartItem.totalPrice.toFixed(2) %> </h5>
  </td>
</tr>